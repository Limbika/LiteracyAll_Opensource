    <color name="yellow_selected">#ffeb3b</color>
    <declare-styleable name="AccesibleStyledSeekBar"><attr format="integer" name="left"/><attr format="integer" name="rigth"/></declare-styleable>
    <declare-styleable name="ToggleImageButton"><attr name="android:checked"/></declare-styleable>
    <declare-styleable name="lockable"><attr format="boolean" name="state_locked"/></declare-styleable>
    <dimen name="dp_00">1dp</dimen>
    <dimen name="dp_01">2dp</dimen>
    <dimen name="dp_02">4dp</dimen>
    <dimen name="dp_03">8dp</dimen>
    <dimen name="dp_04">16dp</dimen>
    <dimen name="dp_05">24dp</dimen>
    <dimen name="dp_06">32dp</dimen>
    <dimen name="dp_07">48dp</dimen>
    <dimen name="dp_08">64dp</dimen>
    <dimen name="dp_09">96dp</dimen>
