<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dialog_bg">

    <LinearLayout
        android:layout_width="@dimen/five_hundred"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/twenty"
        android:orientation="vertical"
        android:weightSum="6.5">

        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight=".5" />


        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="1">

            <com.literacyall.app.customui.CustomTextView
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/twenty"
                android:layout_weight=".2"
                android:gravity="center"
                android:scaleType="fitCenter"
                android:text="@string/Feedback"
                android:textColor="@color/black" />


            <com.literacyall.app.customui.CustomTextView
                android:id="@+id/tvFeedBack"
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/twenty"
                android:layout_weight=".6"
                android:background="@color/white"
                android:gravity="center"
                android:scaleType="fitCenter"
                android:textColor="@color/black" />

            <ImageButton
                android:id="@+id/ibtnFBSoundClose"
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/ten"
                android:layout_weight=".2"
                android:background="@android:color/transparent"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_delete" />

        </LinearLayout>


        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/twenty"
            android:layout_weight=".5" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="1">

            <com.literacyall.app.customui.CustomTextView
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/twenty"
                android:layout_weight=".2"
                android:gravity="center"
                android:scaleType="fitCenter"
                android:text="@string/positive"
                android:textColor="@color/black" />


            <com.literacyall.app.customui.CustomTextView
                android:id="@+id/tvPositive"
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/twenty"
                android:layout_weight=".6"
                android:background="@color/white"
                android:gravity="center"
                android:scaleType="fitCenter"
                android:textColor="@color/black" />

            <ImageButton
                android:id="@+id/ibtnPositiveSoundClose"
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/ten"
                android:layout_weight=".2"
                android:background="@android:color/transparent"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_delete" />

        </LinearLayout>


        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/twenty"
            android:layout_weight=".5" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="1">

            <com.literacyall.app.customui.CustomTextView
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/twenty"
                android:layout_weight=".2"
                android:gravity="center"
                android:scaleType="fitCenter"
                android:text="@string/negative"
                android:textColor="@color/black" />

            <com.literacyall.app.customui.CustomTextView
                android:id="@+id/tvNegative"
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/twenty"
                android:layout_weight=".6"
                android:background="@color/white"
                android:gravity="center"
                android:scaleType="fitCenter"
                android:textColor="@color/black" />

            <ImageButton
                android:id="@+id/ibtnNegativeSoundClose"
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/ten"
                android:layout_weight=".2"
                android:background="@android:color/transparent"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_delete" />

        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight=".5" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="1">

            <View
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_weight=".42"
                android:gravity="center"
                android:scaleType="fitCenter" />

            <ImageButton
                android:id="@+id/ibtnSoundDone"
                android:layout_width="@dimen/zero"
                android:layout_height="@dimen/eighty"
                android:layout_marginRight="@dimen/fifteen"
                android:layout_weight=".16"
                android:background="@android:color/transparent"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_circuler_done" />


            <View
                android:layout_width="@dimen/zero"
                android:layout_height="fill_parent"
                android:layout_weight=".42"
                android:gravity="center"
                android:scaleType="fitCenter" />

        </LinearLayout>


        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/twenty"
            android:layout_weight=".5" />-->

    </LinearLayout>


</RelativeLayout>
